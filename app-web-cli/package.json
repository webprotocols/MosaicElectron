{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" \"cpx \"src/**/*.{html,css,js,bat,png,jpg}\" dist --watch\" \"electron .\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "rebuild": "electron-rebuild -f",
    "cpx": "cpx \"src/**/*.{html,css,js,bat,png,jpg}\" dist",
    "cpx:w": "cpx \"src/**/*.{html,css,,bat,png,jpg}\" dist --watch",
    "electron": "electron .",
    "rm": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.2",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/electron": "^1.4.30",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "cpx": "^1.5.0",
    "electron": "^1.4.13",
    "electron-rebuild": "^1.4.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "lite-server": "^2.2.2",
    "typescript": "~2.0.10"
  },
  "repository": {},
  "main": "electron_main.js"
}
